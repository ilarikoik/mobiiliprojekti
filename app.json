{
  "expo": {
    "name": "leffa",
    "slug": "leffa",
    "version": "1.0.0",
    "newArchEnabled": true,
    "plugins": [
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow Leffa to use your location."
        }
      ],
      "expo-sqlite"
    ],
    "ios": {
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "Sovellus tarvitsee luvan sijainnin käyttämiseen.",
        "NSLocationAlwaysAndWhenInUseUsageDescription": "Sovellus tarvitsee luvan sijainnin käyttämiseen myös silloin, kun se ei ole käytössä."
      },
      "bundleIdentifier": "com.anonymous.leffa"
    },
    "android": {
      "permissions": [
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION",
        "ACCESS_BACKGROUND_LOCATION",
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION"
      ]
    }
  }
}
